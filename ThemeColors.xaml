<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Wiggum">
    <Color x:Key="ThemeBaseColor">#A06C28</Color>
    <SolidColorBrush x:Key="ThemeBaseColorBrush" Color="{ThemeResource ThemeBaseColor}" />
    <Color x:Key="ThemeBaseLighterColor">#FFC06E</Color>
    <SolidColorBrush x:Key="ThemeBaseLighterColorBrush" Color="{ThemeResource ThemeBaseLighterColor}" />
    <Color x:Key="ThemeBaseDarkerColor">#663A00</Color>
    <SolidColorBrush x:Key="ThemeBaseDarkerColorBrush" Color="{ThemeResource ThemeBaseDarkerColor}" />
    <Color x:Key="ThemeAccentColor">#1E4967</Color>
    <SolidColorBrush x:Key="ThemeAccentColorBrush" Color="{ThemeResource ThemeAccentColor}" />
    <Color x:Key="ThemeAccentLighterColor">#5288AF</Color>
    <SolidColorBrush x:Key="ThemeAccentLighterColorBrush" Color="{ThemeResource ThemeAccentLighterColor}" />
    <Color x:Key="ThemeAccentDarkerColor">#012741</Color>
    <SolidColorBrush x:Key="ThemeAccentDarkerColorBrush" Color="{ThemeResource ThemeAccentDarkerColor}" />
</ResourceDictionary>
